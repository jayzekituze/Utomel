task.spawn(function()
    wait(10)
    local player = game.Players.LocalPlayer

    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

    local targetCFrame = CFrame.new(
        -10275.4131, 5.40920258, -7313.2832, 
        0.999543607, 0, 0.0302092619, 
        0, 1, 0, 
        -0.0302092619, 0, 0.999543607
    )

    humanoidRootPart.CFrame = targetCFrame
end)


task.spawn(function()
    wait(10)
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local Network = ReplicatedStorage:WaitForChild("Network")
    local Merchant_RequestPurchase = Network:WaitForChild("Merchant_RequestPurchase")

    local function makePurchase(number)
        local args = {
            [1] = "LuckyDiceMerchant",
            [2] = number
        }

        Merchant_RequestPurchase:InvokeServer(unpack(args))
    end

    while true do
        for number = 1, 6 do
            makePurchase(number)
            wait(1)
        end
        wait(5)
    end
end)

task.spawn(function()
    wait(10)
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local Network = ReplicatedStorage:WaitForChild("Network")
    local Rng_Roll = Network:WaitForChild("Rng_Roll")

    local args = {
        [1] = "First"
    }

    local rollInterval = 0.1 -- Adjust this value to control the speed of rolling (in seconds)

    while true do
        Rng_Roll:InvokeServer(unpack(args))
        wait(rollInterval)
    end
end)


