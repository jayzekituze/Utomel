        FPSBooster = false,
        MinRebirthOrbs = 20 -- Minimum amount of coin orbs to leave to collect once rebirthed (IF YOU DON'T KNOW WHAT THIS DOES, DON'T CHANGE IT)
    }
    
    loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/e050f6fa2a8b1861079735e969b8aeb8.lua"))()
end)
